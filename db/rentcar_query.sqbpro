CREATE TABLE vehicles(
id_vehicle INTEGER PRIMARY KEY AUTOINCREMENT, 
name varchar(55) not null,
model varchar(55) not null,
year date not null,
rent_price decimal(7, 2) not null
);

CREATE TABLE car_rent(
id_crnt INTEGER PRIMARY KEY AUTOINCREMENT, 
id_user INTEGER not null,
id_vehicle INTEGER not null, 
start_date date not null, 
end_date date not null,
FOREIGN KEY (id_user) REFERENCES user(id_user),
FOREIGN KEY (id_vehicle) REFERENCES vechicle(id_vehicle)
);

CREATE TABLE credentials(
id_cre INTEGER PRIMARY KEY AUTOINCREMENT, 
id_user INTEGER not null, 
email varchar(65) not null, 
user_password varchar(25) not NULL,
FOREIGN KEY (id_user) REFERENCES user(id_user)
);


